<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.vo.KKUserMst"  table="kk_user_mst">
      <meta attribute="class-description">
       
      </meta>
      <id name="userId" type="java.lang.Long" column="USER_ID">
         <generator class="native"/>
      </id>
      
   
      <property name="userName" column="user_Name" type="java.lang.String"/>
      <property name="email" column="email" type="java.lang.String"/>
      <property name="mobileNO" column="mobile_NO" type="java.lang.Long"/>
      <property name="password" column="password" type="java.lang.String"/>
      <property name="parentUserId" column="PARENT_USER_ID" type="java.lang.Long"/>
     
     <property name="roleId" column="USER_ROLE" type="java.lang.Integer"/>
      <property name="activeFlag" column="activate_flag" type="java.lang.Integer"/>
      

   </class>

   

</hibernate-mapping>